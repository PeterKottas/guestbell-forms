{
  "version": "1.0.213",
  "description": "React forms with validation as used in GuestBell",
  "keywords": [
    "react",
    "validation",
    "form",
    "react validation",
    "form validation",
    "input validation",
    "react input"
  ],
  "main": "build/guestbell-forms.js",
  "style": "build/guestbell-forms.css",
  "dependencies": {
    "kefir": "^3.8.3",
    "kefir-bus": "^2.2.1"
  },
  "devDependencies": {
    "@types/react": "16.4.13",
    "@types/react-dom": "16.0.7",
    "@types/react-onclickoutside": "6.0.4",
    "@types/webpack": "4.4.11",
    "@types/webpack-env": "1.13.6",
    "autoprefixer": "^9.1.5",
    "awesome-typescript-loader": "5.2.1",
    "cash-true": "0.0.2",
    "connect-history-api-fallback": "1.5.0",
    "conventional-github-releaser": "3.1.2",
    "css-loader": "1.0.0",
    "cssnano": "4.1.0",
    "dts-bundle": "0.7.3",
    "file-loader": "2.0.0",
    "lodash": "4.17.10",
    "material-design-icons": "3.0.1",
    "mini-css-extract-plugin": "0.4.2",
    "moment": "2.22.2",
    "node-sass": "4.9.3",
    "postcss-loader": "3.0.0",
    "prop-types": "15.6.2",
    "react": "16.5.0",
    "react-datepicker": "1.6.0",
    "react-dom": "16.5.0",
    "react-hot-loader": "4.3.6",
    "react-ink": "6.4.0",
    "react-onclickoutside": "6.7.1",
    "react-textarea-autosize": "7.0.4",
    "react-tippy": "1.2.2",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "svg-react-loader": "0.4.5",
    "typescript": "3.0.3",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "1.1.1",
    "webpack": "4.17.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "3.1.8",
    "webpack-hot-middleware": "2.23.1",
    "webpack-merge": "4.1.4"
  },
  "name": "guestbell-forms",
  "scripts": {
    "start": "yarn && set NODE_ENV=local && xcopy src\\demo\\ClientApp\\assets\\favicon\\icons src\\demo\\wwwroot\\dist\\icons /e /i /h /y && webpack-dev-server --config webpack/webpack.config.dev.js --content-base src/demo/wwwroot --hot --inline --open --history-api-fallback",
    "build:clean": "rimraf build/",
    "build:lib": "webpack --config ./webpack/webpack.config.js && webpack --config ./webpack/webpack.config.min.js",
    "build:min": "webpack --config ./webpack/webpack.config.min.js",
    "build": "npm run build:clean && npm run build:lib",
    "build:dev": "set NODE_ENV=development && npm run build",
    "build:prod": "set NODE_ENV=production && npm run build",
    "build:gh-pages": "set NODE_ENV=production && xcopy src\\demo\\wwwroot\\index.html docs /y /e /i /h && webpack --config ./webpack/webpack.config.gh-pages.js && xcopy src\\demo\\ClientApp\\assets\\favicon\\icons docs\\dist\\icons /e /i /h /y",
    "version:patch": "npm version patch",
    "push:git": "git push --tags origin master --quiet",
    "conventional-github-releaser": "conventional-github-releaser -p angular",
    "commit:build": "git add . && git commit -m \"Deploy commit\" --quiet && npm run push:git || true",
    "create:patch": "npm run version:patch && npm run push:git",
    "deploy": ".env.bat && npm run build:gh-pages && npm run build:prod && npm run commit:build && npm publish && npm run conventional-github-releaser -p angular && npm run create:patch"
  },
  "typings": "src/lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterKottas/guestbell-forms.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PeterKottas/guestbell-forms/issues"
  },
  "homepage": "https://github.com/PeterKottas/guestbell-forms",
  "peerDependencies": {
    "moment": "*",
    "prop-types": "*",
    "react": "*",
    "react-datepicker": "*",
    "react-ink": "*",
    "react-onclickoutside": "*",
    "react-textarea-autosize": "*",
    "react-tippy": "*"
  }
}
