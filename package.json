  },
  "homepage": "https://github.com/PeterKottas/guestbell-forms",
  "peerDependencies": {
    "@material-ui/core": "4.x",
    "@tippy.js/react": "3.x",
    "react": "16.x",
    "react-datepicker": "*",
    "react-dom": "*",
    "react-ink": "*",
    "react-onclickoutside": "*",
    "moment": "*"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}
