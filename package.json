{
  "version": "1.0.0",
  "description": "React forms with validation as used in GuestBell",
  "keywords": [
    "react",
    "validation",
    "form",
    "react validation",
    "form validation",
    "input validation",
    "react input"
  ],
  "main": "/build/guestbell-forms.js",
  "dependencies": {
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-hot-loader": "3.0.0-beta.7"
  },
  "devDependencies": {
    "@types/react": "15.0.28",
    "@types/react-dom": "15.5.0",
    "@types/webpack": "2.2.15",
    "@types/webpack-env": "1.13.0",
    "awesome-typescript-loader": "3.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "0.28.4",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "node-sass": "4.5.3",
    "postcss-loader": "2.0.6",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "typescript": "2.3.4",
    "url-loader": "0.5.9",
    "webpack": "3.4.1",
    "webpack-hot-middleware": "2.18.0",
    "webpack-merge": "4.1.0",
    "webpack-dev-server": "2.6.1"
  },
  "name": "guestbell-forms",
  "scripts": {
    "start": "webpack-dev-server --config webpack/webpack.config.dev.js --content-base docs --hot --inline --open --history-api-fallback",
    "build-clean": "rimraf build/",
    "build-code": "webpack --config ./webpack/webpack.config.js && webpack --config ./webpack/webpack.config.min.js",
    "build": "npm run build-clean && npm run build-code",
    "build-dev": "set NODE_ENV=development && npm run build",
    "build-prod": "set NODE_ENV=production && npm run build",
    "build-gh-pages": "set NODE_ENV=production && xcopy  src\\demo\\wwwroot\\index.html docs /y /e /i /h && webpack --config ./webpack/webpack.config.gh-pages.js "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterKottas/guestbell-forms.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PeterKottas/guestbell-forms/issues"
  },
  "homepage": "https://github.com/PeterKottas/guestbell-forms",
  "peerDependencies": {
    "react": "*",
    "prop-type": "*"
  }
}
